{
  "name": "@job/backend",
  "version": "1.0.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "license": "MIT",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "start": "yarn build && yarn serve",
    "serve": "node dist/server.js",
    "serve:dev": "nodemon dist/server.js",
    "watch": "yarn dirs && concurrently \"yarn build:js --watch\" \"yarn serve:dev\"",
    "dirs": "rimraf dist && mkdirp dist && yarn copy-static-assets",
    "build": "yarn build:types && yarn build:js && yarn copy-static-assets",
    "build:types": "tsc -p tsconfig.prod.json --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "test": "NODE_ENV=test jest",
    "test:coverage": "yarn test --coverage --detectOpenHandles",
    "copy-static-assets": "ts-node copyStatic.ts",
    "lint:ts:fix": "eslint . --ext .ts --fix",
    "format:write": "prettier --write . --ignore-path ../../.prettierignore"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.5",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/pdfjs-dist": "^2.1.7",
    "@types/pug": "^2.0.4",
    "@types/redis": "^2.8.28",
    "@types/shelljs": "^0.8.8",
    "@types/yauzl": "^2.9.1",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "shelljs": "0.8.4",
    "ts-node": "9.1.1"
  },
  "repository": {
    "url": "https://github.com/halcika7/JobWeb"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.7.0",
    "@job/common": "1.0.0",
    "@sendgrid/mail": "^7.4.0",
    "axios": "^0.21.0",
    "cloudinary": "^1.23.0",
    "compression": "^1.7.4",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fast-xml-parser": "^3.17.5",
    "helmet": "^4.2.0",
    "hpp": "^0.2.3",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pdfjs-dist": "^2.5.207",
    "pug": "^3.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.3.3",
    "yauzl": "^2.10.0"
  }
}

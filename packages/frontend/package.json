    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-root-import": "^6.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "moxios": "^0.4.0",
    "react-app-rewire-alias": "^0.1.9",
    "react-app-rewired": "^2.1.8",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "serve": "serve -l 3000 -s build",
    "test": "NODE_ENV=test react-app-rewired test --env=jest-environment-jsdom-sixteen --verbose --collectCoverage",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint:ts:fix": "eslint . --ext .ts,.tsx --fix",
    "format:write": "prettier --write . --ignore-path ../../.prettierignore"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "globals": {
      "babel-jest": {
        "tsConfig": "tsconfig.paths.json",
        "diagnostics": true
      },
      "NODE_ENV": "test"
    },
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "collectCoverageFrom": [
      "!src/serviceWorker.ts",
      "!src/index.tsx",
      "!src/routes/**",
      "!src/library/axios.ts",
      "!src/redux/types/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__mocks__"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/src/**/*.test.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>src/__tests__/__mocks__/fileMock.ts"
    }
  }
}
